<launch>

<rosparam file="$(find maze_bot_description)/launch/controller.yaml" command="load"/>
<node name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false" output="screen" ns="maze_bot" args="Revolute 1_position_controller Revolute 2_position_controller joint_state_controller "/>
<node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher" respawn="false" output="screen">
  <remap from="/joint_states" to="/maze_bot/joint_states"/>
</node>

</launch>