<launch>
  <param command="$(find xacro)/xacro $(find differential_drive_robot_description)/urdf/differential_drive_robot.xacro" name="robot_description"/>
  <node args="-param robot_description -urdf -model differential_drive_robot" name="spawn_urdf" pkg="gazebo_ros" type="spawn_model"/>


<node name="joint_state_publisher" pkg = "joint_state_publisher" type ="joint_state_publisher"/>
<node name="robot_state_publisher" pkg = "robot_state_publisher" type ="robot_state_publisher"/>

<include file="$(find gazebo_ros)/launch/empty_world.launch">


    <arg name="world_name" value="$(find differential_drive_robot_description)/worlds/my_frist_world.world"/>
    <arg name="paused" value="false"/>
    <arg name="use_sim_time" value="true"/>
    <arg name="gui" value="true"/>
    <arg name="headless" value="false"/>
    <arg name="debug" value="false"/>
  </include>
</launch>
